	pages = {852--869},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\Ed\\Zotero\\storage\\C38UL9NF\\824414.html:text/html;Versão submetida:C\:\\Users\\Ed\\Zotero\\storage\\HWX5ZDNX\\Lupu e Sloman - 1999 - Conflicts in policy-based distributed systems mana.pdf:application/pdf}
